{
   "name": "hackathon-backend-service",
   "version": "1.0.0",
   "description": "It is a backend service for hackathon project",
   "main": "index.js",
   "type": "module",
   "scripts": {
      "start": "node src/server.js",
      "dev": "cross-env NODE_ENV=dev nodemon src/server.js",
      "format:check": "prettier . --check",
      "format:fix": "prettier . --write",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "prepare": "husky install",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "lint-staged": {
      "*.js": [
         "npm run lint:fix",
         "npm run format:fix"
      ]
   },
   "author": "",
   "license": "ISC",
   "devDependencies": {
      "@types/node": "^16.11.10",
      "eslint": "^8.53.0",
      "eslint-config-prettier": "^9.0.0",
      "husky": "^8.0.3",
      "lint-staged": "^15.0.2",
      "nodemon": "^3.0.1",
      "prettier": "3.0.3",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "cross-env": "^7.0.3",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "http-errors": "^2.0.0",
      "mongodb": "^5.2.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.17",
      "winston": "^3.11.0"
   }
}